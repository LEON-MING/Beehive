./javascripts/js/bootstrap.js: * Bootstrap: dropdown.js v3.0.3
./javascripts/js/bootstrap.js: * http://getbootstrap.com/javascript/#dropdowns
./javascripts/js/bootstrap.js:  var backdrop = '.dropdown-backdrop'
./javascripts/js/bootstrap.js:  var toggle   = '[data-toggle=dropdown]'
